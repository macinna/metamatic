# Agent-specific environment configuration template
# Copy to .env.local or .env.dev and fill in values

# Strava client configuration
# Values: mock, real
STRAVA_CLIENT_MODE=mock

# Logging configuration
# Values: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# Strava API access token (required for real client mode)
# Get from: https://www.strava.com/settings/api
STRAVA_ACCESS_TOKEN=your_strava_access_token_here

# Development settings
DEBUG=true
