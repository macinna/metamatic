# Web/Frontend environment configuration template
# Copy to .env.local or .env.dev and fill in values

# API endpoints
NEXT_PUBLIC_API_URL=http://localhost:3001

# Strava OAuth configuration (public client ID is safe to include)
NEXT_PUBLIC_STRAVA_CLIENT_ID=your_strava_public_client_id

# Environment settings
# Values: local, dev, staging, production
NEXT_PUBLIC_ENVIRONMENT=local

# Development settings
NEXT_PUBLIC_DEBUG=true
